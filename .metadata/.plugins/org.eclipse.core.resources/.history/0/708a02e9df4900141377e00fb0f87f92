package magServer;

class Magazine(int capacity){
	int cap = capacity 
	Tube[] tubes =  new Tube[](capacity);
	
	public void init(){
		for (int i = 0; i<capacity; i++){
			void tubes = new Tube() +: tubes;
		};
	}
	public void rotate(this){
		this.tubes = tubes.add(tubes(0));
		this.tubes = tubes.remove(tubes(0));
		}
	public Tube apply(int num){
		Tube tubes(num);
		}
}